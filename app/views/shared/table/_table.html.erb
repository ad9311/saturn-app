<table class="w-max">
  <thead>
    <tr>
      <% table_columns.each do |column| %>
        <th><%= column %></th>
      <% end %>
    </tr>
  </thead>
  <tbody>
    <% table_rows.each do |rows| %>
      <tr>
        <% rows.each_with_index do |cell, index| %>
          <td>
            <% if cell[:partial].nil? %>
              <%= cell[:data] %>
            <% else %>
              <%= render cell[:partial], **cell[:options] %>
            <% end %>
          </td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>
