<%= render "shared/form_errors", record: @income %>
<%= form_with url: @form_url, model: @income do |f| %>
  <div>
    <%= f.label :description %>
    <%= f.text_area :description %>
  </div>
  <div>
    <%= f.label :amount %>
    <%= f.number_field :amount %>
  </div>
  <%= f.submit 'Submit' %>
<% end %>
